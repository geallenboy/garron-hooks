#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/sn/Desktop/work/study/garron-hooks/node_modules/.pnpm/@umijs+test@3.5.21/node_modules/@umijs/test/bin/node_modules:/Users/sn/Desktop/work/study/garron-hooks/node_modules/.pnpm/@umijs+test@3.5.21/node_modules/@umijs/test/node_modules:/Users/sn/Desktop/work/study/garron-hooks/node_modules/.pnpm/@umijs+test@3.5.21/node_modules/@umijs/node_modules:/Users/sn/Desktop/work/study/garron-hooks/node_modules/.pnpm/@umijs+test@3.5.21/node_modules:/Users/sn/Desktop/work/study/garron-hooks/node_modules/.pnpm/node_modules:/Users/sn/Desktop/work/study/garron-hooks/node_modules:/Users/sn/Desktop/work/study/node_modules:/Users/sn/Desktop/work/node_modules:/Users/sn/Desktop/node_modules:/Users/sn/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/sn/Desktop/work/study/garron-hooks/node_modules/.pnpm/@umijs+test@3.5.21/node_modules/@umijs/test/bin/node_modules:/Users/sn/Desktop/work/study/garron-hooks/node_modules/.pnpm/@umijs+test@3.5.21/node_modules/@umijs/test/node_modules:/Users/sn/Desktop/work/study/garron-hooks/node_modules/.pnpm/@umijs+test@3.5.21/node_modules/@umijs/node_modules:/Users/sn/Desktop/work/study/garron-hooks/node_modules/.pnpm/@umijs+test@3.5.21/node_modules:/Users/sn/Desktop/work/study/garron-hooks/node_modules/.pnpm/node_modules:/Users/sn/Desktop/work/study/garron-hooks/node_modules:/Users/sn/Desktop/work/study/node_modules:/Users/sn/Desktop/work/node_modules:/Users/sn/Desktop/node_modules:/Users/sn/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/umi-test.js" "$@"
else
  exec node  "$basedir/../../bin/umi-test.js" "$@"
fi
