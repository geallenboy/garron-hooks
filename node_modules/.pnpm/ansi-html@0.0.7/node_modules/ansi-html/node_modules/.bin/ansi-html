#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/sn/Desktop/work/study/garron-hooks/node_modules/.pnpm/ansi-html@0.0.7/node_modules/ansi-html/bin/node_modules:/Users/sn/Desktop/work/study/garron-hooks/node_modules/.pnpm/ansi-html@0.0.7/node_modules/ansi-html/node_modules:/Users/sn/Desktop/work/study/garron-hooks/node_modules/.pnpm/ansi-html@0.0.7/node_modules:/Users/sn/Desktop/work/study/garron-hooks/node_modules/.pnpm/node_modules:/Users/sn/Desktop/work/study/garron-hooks/node_modules:/Users/sn/Desktop/work/study/node_modules:/Users/sn/Desktop/work/node_modules:/Users/sn/Desktop/node_modules:/Users/sn/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/sn/Desktop/work/study/garron-hooks/node_modules/.pnpm/ansi-html@0.0.7/node_modules/ansi-html/bin/node_modules:/Users/sn/Desktop/work/study/garron-hooks/node_modules/.pnpm/ansi-html@0.0.7/node_modules/ansi-html/node_modules:/Users/sn/Desktop/work/study/garron-hooks/node_modules/.pnpm/ansi-html@0.0.7/node_modules:/Users/sn/Desktop/work/study/garron-hooks/node_modules/.pnpm/node_modules:/Users/sn/Desktop/work/study/garron-hooks/node_modules:/Users/sn/Desktop/work/study/node_modules:/Users/sn/Desktop/work/node_modules:/Users/sn/Desktop/node_modules:/Users/sn/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/ansi-html" "$@"
else
  exec node  "$basedir/../../bin/ansi-html" "$@"
fi
