#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/sn/Desktop/work/study/garron-hooks/node_modules/.pnpm/speech-rule-engine@3.3.3/node_modules/speech-rule-engine/bin/node_modules:/Users/sn/Desktop/work/study/garron-hooks/node_modules/.pnpm/speech-rule-engine@3.3.3/node_modules/speech-rule-engine/node_modules:/Users/sn/Desktop/work/study/garron-hooks/node_modules/.pnpm/speech-rule-engine@3.3.3/node_modules:/Users/sn/Desktop/work/study/garron-hooks/node_modules/.pnpm/node_modules:/Users/sn/Desktop/work/study/garron-hooks/node_modules:/Users/sn/Desktop/work/study/node_modules:/Users/sn/Desktop/work/node_modules:/Users/sn/Desktop/node_modules:/Users/sn/node_modules:/Users/node_modules:/node_modules:/Users/sn/Desktop/work/study/garron-hooks/node_modules/.pnpm/node_modules/speech-rule-engine/bin/node_modules:/Users/sn/Desktop/work/study/garron-hooks/node_modules/.pnpm/node_modules/speech-rule-engine/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/sn/Desktop/work/study/garron-hooks/node_modules/.pnpm/speech-rule-engine@3.3.3/node_modules/speech-rule-engine/bin/node_modules:/Users/sn/Desktop/work/study/garron-hooks/node_modules/.pnpm/speech-rule-engine@3.3.3/node_modules/speech-rule-engine/node_modules:/Users/sn/Desktop/work/study/garron-hooks/node_modules/.pnpm/speech-rule-engine@3.3.3/node_modules:/Users/sn/Desktop/work/study/garron-hooks/node_modules/.pnpm/node_modules:/Users/sn/Desktop/work/study/garron-hooks/node_modules:/Users/sn/Desktop/work/study/node_modules:/Users/sn/Desktop/work/node_modules:/Users/sn/Desktop/node_modules:/Users/sn/node_modules:/Users/node_modules:/node_modules:/Users/sn/Desktop/work/study/garron-hooks/node_modules/.pnpm/node_modules/speech-rule-engine/bin/node_modules:/Users/sn/Desktop/work/study/garron-hooks/node_modules/.pnpm/node_modules/speech-rule-engine/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../speech-rule-engine/bin/sre" "$@"
else
  exec node  "$basedir/../speech-rule-engine/bin/sre" "$@"
fi
